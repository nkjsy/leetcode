class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        l = []
        while x != 0:
            l.append(x % 10)
            x = x // 10
        d = 1
        rev = 0
        l.reverse()
        for i in l:
            rev += d * i
            d *= 10
        return rev
